<h1>Current Signups</h1>

<table>
  <tr>
    <th>Premium</th>
    <th>Email</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Full name</th>
    <th>Folio name</th>
    <th>About me</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.premium %></td>
    <td><%= user.email %></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.full_name %></td>
    <td><%= user.folio_name %></td>
    <td><%= user.about_me %></td>
    <td><%= link_to 'Show', user %></td>
  </tr>
<% end %>
</table>

